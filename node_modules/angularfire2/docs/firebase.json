{
  "firebase": "angular-fire-2",
  "public": ".",
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "type": 302
    }
  ],
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "functions": {
"predeploy": [
    "npm --prefix ./functions run lint",
  ]
},
"database": {
"rules": "database.rules.json"
}
}
